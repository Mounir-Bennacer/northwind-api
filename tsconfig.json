{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": "./",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["./src/*"],
      "@/routes": ["./src/routes/*"],
      "@/lib": ["./src/lib/*"]
    },
    "types": ["node"],
    "strict": true,
    "outDir": "./dist",
    "skipLibCheck": true
  }
}
